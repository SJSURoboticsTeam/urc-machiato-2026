# Execute a complete mission sequence

# Goal: Mission parameters
string mission_type  # Type of mission (e.g., "sample_collection", "scouting")
string mission_id    # Unique mission identifier
string[] waypoints   # List of waypoints to visit (optional)
float32 timeout      # Maximum mission time (seconds, 0 = no limit)

---
# Feedback: Mission progress
string current_phase     # Current mission phase
float32 progress         # Overall progress (0.0 to 1.0)
string status_message    # Human-readable status
int32 waypoints_completed # Number of waypoints completed
float32 estimated_time_remaining # Seconds until completion

---
# Result: Mission outcome
bool success
string completion_status  # Detailed completion message
string[] completed_tasks  # List of tasks that were completed
float32 total_time        # Total mission time in seconds
int32 waypoints_visited   # Total waypoints visited
